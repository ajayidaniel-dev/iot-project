{
  "expo": {
    "name": "my-iot-app",
    "slug": "my-iot-app",
    "scheme": "my-iot-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ventilation.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/ven.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5dbdb"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "/assets/fonts/Sora-Regular.ttf",
            "/assets/fonts/Sora-SemiBold.ttf",
            "/assets/fonts/Sora-SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
